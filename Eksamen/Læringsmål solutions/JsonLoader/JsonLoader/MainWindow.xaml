<Window x:Class="JsonLoader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JsonLoader"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="257.3"
        SizeToContent="Height">
    <Grid>
        <StackPanel>
            <Button Content="Click" Click="ButtonBase_OnClick" Margin="5"></Button>
            <ListView Name="ListView" ItemsSource="{Binding Source={StaticResource Derp}}"
                      Margin="5" MinHeight="100"></ListView>
        </StackPanel>
    </Grid>
</Window>
