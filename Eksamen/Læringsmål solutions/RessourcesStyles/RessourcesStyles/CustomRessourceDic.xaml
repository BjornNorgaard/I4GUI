<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RessourcesStyles">






    <!--Beginning of styles-->
    <Style TargetType="{x:Type Button}">
        <Setter Property="Background" Value="SeaGreen" />
        <Setter Property="Foreground" Value="AliceBlue" />
        <Setter Property="Margin" Value="5"></Setter>
    </Style>

    <Style TargetType="{x:Type Image}">
        <Setter Property="Margin" Value="5"></Setter>
    </Style>

    <Style TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="Courier New"></Setter>
    </Style>
    <!--End of styles-->









    <!--denne skal også applies inde i xaml for hoved vinduet-->
    <Style TargetType="Window" x:Key="MyWindowStyle">
        <Setter Property="Background" Value="AntiqueWhite"></Setter>
    </Style>












    <!--Begin something else-->
    <!--
    <SolidColorBrush x:Key="myBrush" Color="Coral"></SolidColorBrush>
    -->
    <!--End something else-->























</ResourceDictionary>