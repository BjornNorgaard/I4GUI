<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:I4GUI_eksamen_2016_sommer">
    
    <Style TargetType="Button">
        <Setter Property="Margin" Value="5"></Setter>
        <Setter Property="Background" Value="DarkSeaGreen"></Setter>
    </Style>
    
    <Style TargetType="TextBlock">
        <Setter Property="Margin" Value="5,5,5,0"></Setter>
    </Style>
    
    <Style TargetType="TextBox">
        <Setter Property="Margin" Value="5,0,5,5"></Setter>
    </Style>
    
    <Style TargetType="ListBox">
        <Setter Property="Margin" Value="5"></Setter>
    </Style>
    
    <Style TargetType="Window" x:Key="MyStyle">
        <Setter Property="Background" Value="BlanchedAlmond"></Setter>
    </Style>
    
</ResourceDictionary>